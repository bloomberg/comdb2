explain query plan SELECT i.work_item_id FROM WorkItemAnalyst a join WorkItem i on i.work_item_id = a.work_item_id join Source s on s.source_id = i.source_id WHERE a.analyst_id = 14624 AND a.date_finished = -1 AND i.closed_on = -1 AND i.state_id = 2002 AND s.deleted_on = -1 and s.product_id in (18147,18148,18149) ORDER BY ( case i.work_item_type_id when 16166 then 0 when 16165 then 1000 else 9000 end + case i.work_item_sub_type_id when 20360 then 0 when 9 then 100 when 4 then 200 else 900 end + case when s.rank = 0 then 90 when s.rank >= 3 then s.rank+10 else s.rank end), i.created_on limit 1;
