explain query plan SELECT tu.trade_update_id, tu.update_id, tu.transaction_history_id, tu.investment_change, tu.cost_set, tu.psell_cost_set, tu.cost_price, tu.psell_cost_price, th.uuid, th.rcv_recomm_id, th.tx_tsp, rcvd.orig_sender_uuid, rcvd.accumulated_investment, rcvd.accumulated_commission, rcvd.shares, rcvd.psell_shares, rcvd.open_price_set, rcvd.psell_open_price_set, rcvd.open_first_tsp, rcvd.psell_open_first_tsp, rec.recommendation_id, rec.sender_uuid, rec.security_id, rec.pbuy_crncy_conv, rec.psell_security_id, rec.psell_crncy_conv, sec.parsekey_sec_id, sec.security_type, psell_sec.parsekey_sec_id AS psell_parsekey_sec_id, psell_sec.security_type AS psell_security_type FROM trade_update tu JOIN transaction_history th USING (transaction_history_id) JOIN rcv_recommendation rcvd USING (rcv_recomm_id) JOIN recommendation rec USING (recommendation_id) JOIN security sec USING (security_id) LEFT JOIN security psell_sec ON psell_security_id = psell_sec.security_id WHERE (tu.cost_set = 0 OR tu.psell_cost_set = 0) AND rcvd.open_price_set=1 AND (rec.psell_security_id IS NULL OR (rec.psell_security_id IS NOT NULL AND rcvd.psell_open_price_set=1)) ORDER BY tu.trade_update_id;
