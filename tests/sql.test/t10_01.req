# GRANT read permission to a long quoted table name (DRQS 162451152)
DROP TABLE IF EXISTS t1;
GRANT READ ON 't1' TO 'default';

DROP TABLE IF EXISTS t123456789012345678901234567890;
GRANT READ ON 't123456789012345678901234567890' TO 'default';

# Test bad table name (name too long)
GRANT READ ON t1234567890123456789012345678901 TO 'default';
GRANT READ ON 't1234567890123456789012345678901' TO 'default';

CREATE TABLE t123456789012345678901234567890(i INT)$$
GRANT READ ON 't123456789012345678901234567890' TO 'default';
DROP TABLE t123456789012345678901234567890;
