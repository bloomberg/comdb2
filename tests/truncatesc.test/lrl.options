nowatch
logmsg level info
enable_new_snapshot
enable_snapshot_isolation
berkattr abort_on_replicant_log_write 1
init_with_genid48
logdelete_lock_trace
verbose_set_sc_in_progress on
physrep_register_interval 30
verbose_physrep on
#setattr REP_PROCESSORS 0
#setattr REP_WORKERS 0
online_recovery on
ack_trace
