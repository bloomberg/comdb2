create table t13(a int primary key, b int); $$
insert into t13 select value, value*2 from generate_series(1, 1000);
create procedure t13 version '1' {
local function run(sql)
    local stmt, rc = db:prepare(sql)
    if rc ~= 0 then
        return -201, "huh?" .. db:error() .. " sql: " .. sql .. " rc " .. rc
    end
    local row = stmt:fetch()
    while row ~= nil do
        row = stmt:fetch()
    end
    stmt:close()
    return 0
end


local function main()
    db:num_columns(0)
    db:setmaxinstructions(100000000)
    for i=1, 10 do
        local rc, err = run("select * from t13")
        if rc ~= 0 then
            return rc, err
        end
        rc, err = run("select sleep(1)")
        if rc ~= 0 then
            return rc, err
        end
    end
end
}
