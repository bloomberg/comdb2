#!/usr/bin/env bash
export TESTID=$1
if [ -z $TESTID ] ; then
    echo "Need testid to kill all tests with this testid"
    exit 1
fi


for test in *.test; do
   export TESTCASE="$(echo $test | sed 's/.test$//g; s///g')"
   export TESTDIR=${PWD}/test_${TESTID}
   DBNAME=${TESTCASE}${TESTID} ./unsetup
done
