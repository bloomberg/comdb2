nowatch
logmsg level info
maxosqltransfer 1000000
enable_new_snapshot
enable_snapshot_isolation
berkattr abort_on_replicant_log_write 1
init_with_genid48

reorder_socksql_no_deadlock on
#reorder_idx_writes on

#selectv_writelock_on_update is enabled by default
#selectv_writelock_on_update on
#selectv_writelock_on_update off
#selectv_writelock on
#reorder_socksql_no_deadlock off
#reorder_idx_writes off
use_modsnap_for_snapshot
