#!/usr/bin/env bash
bash -n "$0" | exit 1

export debug=1

[[ "$debug" == "1" ]] && set -x

x=$($CDB2SQL_EXE --tabs $CDB2_OPTIONS $DBNAME default "exec procedure sys.cmd.send('sc_version_test')")
echo "$x"
echo "$x" | egrep "Schema change version test passed"
if [[ $? -ne 0 ]]; then
    echo "Failed sc-version test"
    exit 1
fi
echo "Success!"
